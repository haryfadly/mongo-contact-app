<h1>Hello !</h1>
<h2>Nama saya <%= nama %>.</h2>
<p>Welcome to my site...</p>

<h4>Daftar mahasiswa.</h4>
<!-- ketik ejsif untuk memanggil template if -->

<% if (mahasiswa.length < 1) { %>
    <p>Data mahasiswa masih kosong.</p>
<% } else {%> 

    ketik ejseach untuk memanggil template for each
<% mahasiswa.forEach(mhs => { %>
    <ul>
        <li>Nama: <%= mhs.nama%> </li>
        <li>Email: <%= mhs.email%></li>
        <li>No handphone: <%= mhs.nohp%></li>
    </ul>
<% }) %>
<% } %> 

